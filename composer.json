{
  "name": "eonx-com/test-utils",
  "description": "A collection of PHP test utils, assertions stubs and test cases",
  "type": "library",
  "license": "BSD-3-Clause",
  "minimum-stability": "stable",
  "require": {
    "php": "^7.3",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "phpunit/phpunit": "^8.4 || ^9.0"
  },
  "require-dev": {
    "doctrine/orm": "^2.7",
    "eonx-com/apiformats": "^1.0",
    "eonx-com/externals": "^1.6",
    "eonx-com/requesthandlers": "^1.2",
    "eonx-com/search": "^3.0",
    "eonx-com/standards": "^0.3",
    "phpmd/phpmd": "^2.7",
    "phpstan/phpstan": "^0.12.3",
    "phpstan/phpstan-phpunit": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12",
    "phpunit/phpunit": "^8.4",
    "sebastian/phpcpd": "^4.1 || ^5.0",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/http-foundation": "^4.2 || ^5.0",
    "symfony/validator": "^4.4 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "Eonx\\TestUtils\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Eonx\\TestUtils\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.x-dev"
    }
  },
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
